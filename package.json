{
  "name": "create-tpl",
  "version": "0.0.7",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bin": {
    "create-tpl": "index.js",
    "cta": "index.js"
  },
  "files": [
    "index.js",
    "config/*",
    "tpls/*",
    "README.md",
    "LICENSE"
  ],
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "np --no-cleanup --no-yarn --yolo --no-publish --any-branch --no-verify",
    "debug": "export CREATE_DEBUG=false && node ./debug.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . --lerna-package create-tpl",
    "release": "node updateVersions"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@deepjs/lint": "^0.0.16",
    "kolorist": "^1.5.0",
    "minimist": "^1.2.5",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "np": "^7.6.0"
  }
}
