{
  "name": "create-tpl",
  "version": "0.0.10",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bin": {
    "create-tpl": "index.js",
    "cta": "index.js"
  },
  "files": [
    "index.js",
    "config.js",
    "tpls/*",
    "README.md",
    "LICENSE"
  ],
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "npm pack --dry-run && np --no-cleanup --no-yarn --yolo --no-publish --any-branch --no-verify",
    "debug": "export CREATE_DEBUG=false && node ./debug.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . --lerna-package create-tpl",
    "test": "jest",
    "down": "node ./download.js",
    "release": "node updateVersions"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "download": "^8.0.0",
    "kolorist": "^1.5.0",
    "minimist": "^1.2.5",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@deepjs/lint": "^0.0.16",
    "@jest/types": "^27.4.2",
    "@types/jest": "^27.0.3",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.3",
    "np": "^7.6.0",
    "ts-jest": "^27.0.7"
  }
}
